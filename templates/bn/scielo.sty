\makeatletter
\renewcommand*{\@fnsymbol}[1]{%
  % Änderungen:
  % * Symbol "**" an Position 2 verschoben.
  % * Symbol "***" eingefügt.
  % * Symbole mit \dagger entfernt.
  \ensuremath{%
    \ifcase#1%
    \or
      {*}%
    \or
      \ddagger
    \or
      \mathsection
    \or
      \mathparagraph
    \or
      \ddagger\ddagger
    \else
      \@ctrerr
    \fi
  }%
}

